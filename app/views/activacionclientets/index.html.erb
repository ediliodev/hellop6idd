<p id="notice"><%= notice %></p>

<h1>Activacionclientets</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Clienteid</th>
      <th>Tipocliente</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @activacionclientets.each do |activacionclientet| %>
      <tr>
        <td><%= activacionclientet.email %></td>
        <td><%= activacionclientet.clienteid %></td>
        <td><%= activacionclientet.tipocliente %></td>
        <td><%= link_to 'Show', activacionclientet %></td>
        <td><%= link_to 'Edit', edit_activacionclientet_path(activacionclientet) %></td>
        <td><%= link_to 'Destroy', activacionclientet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activacionclientet', new_activacionclientet_path %>
