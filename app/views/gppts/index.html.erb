<p id="notice"><%= notice %></p>

<h1>Gppts</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Q1</th>
      <th>Q2</th>
      <th>Q3</th>
      <th>P12</th>
      <th>P13</th>
      <th>P23</th>
      <th>Sp</th>
      <th>T123</th>
      <th>T13</th>
      <th>T23</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gppts.each do |gppt| %>
      <tr>
        <td><%= gppt.nombre %></td>
        <td><%= gppt.q1 %></td>
        <td><%= gppt.q2 %></td>
        <td><%= gppt.q3 %></td>
        <td><%= gppt.p12 %></td>
        <td><%= gppt.p13 %></td>
        <td><%= gppt.p23 %></td>
        <td><%= gppt.sp %></td>
        <td><%= gppt.t123 %></td>
        <td><%= gppt.t13 %></td>
        <td><%= gppt.t23 %></td>
        <td><%= link_to 'Show', gppt %></td>
        <td><%= link_to 'Edit', edit_gppt_path(gppt) %></td>
        <td><%= link_to 'Destroy', gppt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gppt', new_gppt_path %>
