<p id="notice"><%= notice %></p>

<h1>Empleadots</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Cedula</th>
      <th>Telefono</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @empleadots.each do |empleadot| %>
      <tr>
        <td><%= empleadot.nombre %></td>
        <td><%= empleadot.apellido %></td>
        <td><%= empleadot.cedula %></td>
        <td><%= empleadot.telefono %></td>
        <td><%= link_to 'Show', empleadot %></td>
        <td><%= link_to 'Edit', edit_empleadot_path(empleadot) %></td>
        <td><%= link_to 'Destroy', empleadot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Empleadot', new_empleadot_path %>
