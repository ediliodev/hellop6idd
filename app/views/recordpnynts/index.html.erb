<p id="notice"><%= notice %></p>

<h1>Recordpnynts</h1>

<table>
  <thead>
    <tr>
      <th>Ticket</th>
      <th>N1</th>
      <th>N2</th>
      <th>Siglas</th>
      <th>Monto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @recordpnynts.each do |recordpnynt| %>
      <tr>
        <td><%= recordpnynt.ticket %></td>
        <td><%= recordpnynt.n1 %></td>
        <td><%= recordpnynt.n2 %></td>
        <td><%= recordpnynt.siglas %></td>
        <td><%= recordpnynt.monto %></td>
        <td><%= link_to 'Show', recordpnynt %></td>
        <td><%= link_to 'Edit', edit_recordpnynt_path(recordpnynt) %></td>
        <td><%= link_to 'Destroy', recordpnynt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Recordpnynt', new_recordpnynt_path %>
