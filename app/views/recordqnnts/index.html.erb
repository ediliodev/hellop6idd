<p id="notice"><%= notice %></p>

<h1>Recordqnnts</h1>

<table>
  <thead>
    <tr>
      <th>Ticket</th>
      <th>N1</th>
      <th>Monto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @recordqnnts.each do |recordqnnt| %>
      <tr>
        <td><%= recordqnnt.ticket.id %></td>
        <td><%= recordqnnt.n1 %></td>
        <td><%= recordqnnt.monto %></td>
        <td><%= link_to 'Show', recordqnnt %></td>
        <td><%= link_to 'Edit', edit_recordqnnt_path(recordqnnt) %></td>
        <td><%= link_to 'Destroy', recordqnnt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Recordqnnt', new_recordqnnt_path %>
