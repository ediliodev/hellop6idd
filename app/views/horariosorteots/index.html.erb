<p id="notice"><%= notice %></p>

<h1>Horariosorteots</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Lunes</th>
      <th>Martes</th>
      <th>Miercoles</th>
      <th>Jueves</th>
      <th>Viernes</th>
      <th>Sabado</th>
      <th>Domingo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @horariosorteots.each do |horariosorteot| %>
      <tr>
        <td><%= horariosorteot.nombre %></td>
        <td><%= horariosorteot.lunes %></td>
        <td><%= horariosorteot.martes %></td>
        <td><%= horariosorteot.miercoles %></td>
        <td><%= horariosorteot.jueves %></td>
        <td><%= horariosorteot.viernes %></td>
        <td><%= horariosorteot.sabado %></td>
        <td><%= horariosorteot.domingo %></td>
        <td><%= link_to 'Show', horariosorteot %></td>
        <td><%= link_to 'Edit', edit_horariosorteot_path(horariosorteot) %></td>
        <td><%= link_to 'Destroy', horariosorteot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Horariosorteot', new_horariosorteot_path %>
